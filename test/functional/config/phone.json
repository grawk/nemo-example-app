{
  "plugins": {
    "eyes": {
      "module": "path:plugin/eyes",
      "arguments": [{
        "sdk": {
          "setApiKey": "env:applitools_api_key",
          "setBatch": "PHONE",
          "setSaveNewTests": true,
          "setForceFullPageScreenshot": true,
          "setMatchLevel": "Layout"
        },
        "viewport": {
          "width": 620,
          "height": 400
        }
      }]
    }
  }
}
